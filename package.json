{
  "name": "protractor-commonjs-mocha-chai",
  "version": "1.0.0",
  "description": "Protractor end-to-end test automation boilerplate project. Using commonjs, mocha & chai.",
  "scripts": {
    "test": "npx protractor protractor.conf.js",
    "test-ci": "npx protractor protractor-ci.conf.js",
    "upadte-webderiver": "npx webdriver-manager update",
    "lint": "npx eslint --ext .js ./ --fix"
  },
  "keywords": [
    "protractor",
    "commonjs",
    "mocha",
    "chai",
    "boilerplate",
    "example"
  ],
  "author": "Girma Nigusse <xgirma@gmail.com> (https://github.com/xgirma)",
  "license": "MIT",
  "devDependencies": {
    "chai": "4.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.1",
    "mocha": "7.0.1",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "protractor": "5.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "puppeteer": "2.0.0"
  }
}
